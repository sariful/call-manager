<?xml version='1.0' encoding='utf-8'?>
<widget id="com.zubizi.call-manager" version="1.0.9" 
	xmlns="http://www.w3.org/ns/widgets" 
	xmlns:cdv="https://zubizi.com">
	<name>Call Manager</name>
	<description>
        A Simplest call manager
	</description>
	<author email="me@sarifulislam.com" href="https://zubizi.com">
        Zubizi Web Solutions
	</author>
	<content src="index.html" />
	<plugin name="cordova-plugin-whitelist" spec="1" />
	<access origin="*" />
	<allow-intent href="http://*/*" />
	<allow-intent href="https://*/*" />
	<allow-intent href="tel:*" />
	<allow-intent href="sms:*" />
	<allow-intent href="mailto:*" />
	<allow-intent href="geo:*" />
	<platform name="android">
	    <icon src="res/icons/icon.png" />
		<allow-intent href="market:*" />
	</platform>
	<platform name="ios">
		<allow-intent href="itms:*" />
		<allow-intent href="itms-apps:*" />
	</platform>
</widget>
